#include <stdio.h>
#include <string.h>

int main()
{
	char s1[] = "abc";
	char s2[] = "Abcd";
	int flag = strncmp(s1, s2, 3);//指定比较
	
	if(flag > 0)
	{
		printf("[%s] > [%s]\n", s1, s2);
	}
	else if(flag < 0)
	{
		printf("[%s] < [%s]\n", s1, s2);
	}
	else
	{
		printf("[%s] == [%s]\n", s1, s2);
	}
	
	
	return 0;
}
